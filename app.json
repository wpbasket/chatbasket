{
  "expo": {
    "name": "Chatbasket",
    "slug": "chatbasket",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon.png",
    "scheme": "chatbasket",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tasktoclear.chatbasket"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "resize",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.tasktoclear.chatbasket",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "chatbasket.live",
              "pathPrefix": "/public"
            },
            {
              "scheme": "https",
              "host": "chatbasket.live",
              "pathPrefix": "/personal"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Select photos to share with friends or post publicly on your feed.",
          "cameraPermission": "Take photos to share privately with friends or publicly with everyone.",
          "microphonePermission": "Record videos to share on your personal or public timeline."
        }
      ],
      "expo-secure-store",
      "expo-web-browser",
      "expo-notifications",
      [
        "expo-share-intent",
        {
          "iosActivationRules": {
            "NSExtensionActivationSupportsWebURLWithMaxCount": 1,
            "NSExtensionActivationSupportsWebPageWithMaxCount": 1,
            "NSExtensionActivationSupportsImageWithMaxCount": 10,
            "NSExtensionActivationSupportsMovieWithMaxCount": 10,
            "NSExtensionActivationSupportsFileWithMaxCount": 10
          },
          "androidIntentFilters": [
            "*/*",
            "text/*",
            "image/*",
            "video/*",
            "audio/*",
            "application/*"
          ],
          "androidMultiIntentFilters": [
            "*/*",
            "image/*",
            "video/*",
            "audio/*"
          ]
        }
      ],
      "expo-sqlite",
      "expo-video",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "879eefea-0f12-4f91-9b5d-fbe1bb11d047"
      }
    }
  }
}
